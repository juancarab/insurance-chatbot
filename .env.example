# Backend formatter strategy: mock | langchain | gemini
INSURANCE_CHATBOT_FORMATTER=mock

# LangChain integration (required when formatter=langchain)
INSURANCE_CHATBOT_LANGCHAIN_RUNNER=backend.app.langchain_runner:run_langchain_agent

# Gemini integration (required when formatter=gemini)
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-1.5-flash-latest
GEMINI_TEMPERATURE=0.2
GEMINI_TOP_P=0.95
GEMINI_MAX_OUTPUT_TOKENS=1024

# Frontend default backend URL override
INSURANCE_CHATBOT_API_URL=http://localhost:8000/chat
